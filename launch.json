{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "dqn inv_pendulum",
            "type": "python",
            "request": "launch",
            "env": {
                "PYTHONPATH": "/mnt/abolychev/rcognita-research-calf/rcognita"
            },
            "program": "/mnt/abolychev/rcognita-research-calf/rcognita/presets/preset_endpoint.py",
            "args": [
                "system=inv_pendulum",
                "controller=dqn",
                "scenario.N_episodes=2",
                "simulator.time_final=0.2",
                "scenario.is_playback=false",
                "controller.actor.discount_factor=.99",
                "controller.critic.model.dim_hidden=50",
                "controller.sampling_time=0.1",
                "controller.critic.optimizer.opt_options.lr=0.001",
                "controller.critic.optimizer.iterations=1",
                "controller.critic.model.force_positive_def=false",
                "controller.critic.data_buffer_size=152",
                "controller.critic.batch_size=20",
                "controller.critic.td_n=50",
                "controller.critic.model.bias=true",
                "controller/critic=dqn_greedy",
                "controller.actor.epsilon_greedy=true",
                "disallow_uncommitted=false"
            ],
            "console": "integratedTerminal",
            "justMyCode": true
        },
        {
            "name": "mpc inv_pendulum",
            "type": "python",
            "request": "launch",
            "env": {
                "PYTHONPATH": "/mnt/abolychev/rcognita-research-calf/rcognita"
            },
            "program": "/mnt/abolychev/rcognita-research-calf/rcognita/presets/preset_endpoint.py",
            "args": [
                "system=inv_pendulum",
                "controller=mpc",
                "initial_conditions=ic_inv_pendulum_stochastic",
                "controller.actor.prediction_horizon=5",
                "scenario.is_playback=false",
                "disallow_uncommitted=false"
            ],
            "console": "integratedTerminal",
            "justMyCode": true
        },
        {
            "name": "Debug inv_pendulum DDPG",
            "type": "python",
            "request": "launch",
            "program": "preset_endpoint.py",
            "console": "integratedTerminal",
            "justMyCode": true,
            "args": [
                "controller=ddpg",
                "scenario=episodic_reinforce",
                "scenario.is_playback=false",
                "simulator.time_final=10",
                "scenario.N_episodes=3",
                "system=inv_pendulum",
                "disallow_uncommitted=False",
                "initial_conditions=ic_inv_pendulum_stochastic",
                "controller.critic.optimizer.opt_options.lr=0.1",
                "--single-thread",
            ],
            "cwd": "${fileDirname}/../presets",
            "env": {
                "HYDRA_FULL_ERROR": "1",
                "PYTHONPATH": "${fileDirname}/..",
            }
        },
        {
            "name": "Debug inv_pendulum ACPG",
            "type": "python",
            "request": "launch",
            "program": "preset_endpoint.py",
            "console": "integratedTerminal",
            "justMyCode": true,
            "args": [
                "controller=acpg",
                "scenario=episodic_reinforce",
                "scenario.is_playback=false",
                "simulator.time_final=10",
                "scenario.N_episodes=20",
                "system=inv_pendulum",
                "disallow_uncommitted=False",
                "initial_conditions=ic_inv_pendulum_stochastic",
                "controller.critic.optimizer.opt_options.lr=0.00001",
                "controller.critic.td_n=10",
                "controller.critic.batch_size=50",
                "controller.actor.model.diag_scale_coef=0.5",
                "controller.critic.discount_factor=0.94"
            ],
            "cwd": "${fileDirname}/../presets",
            "env": {
                "HYDRA_FULL_ERROR": "1",
                "PYTHONPATH": "${fileDirname}/..",
            }
        }
    ]
}